{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "Issuer": "ecommerce-api",
    "Audience": "ecommerce-client",
    "SecretKey": "your-super-secret-key-that-is-at-least-32-characters-long-change-in-production",
    "ExpirationInMinutes": 60
  },
  "RateLimitOptions": {
    "PermitLimit": 100,
    "WindowInSeconds": 60,
    "QueueLimit": 2
  }
}
